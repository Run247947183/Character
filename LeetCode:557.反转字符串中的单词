class Solution {
    public String reverseWords(String s) {
        char[] arr = s.toCharArray();
        int start = 0;
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == ' ') {
                swap(arr, start, i - 1);
                start = i + 1;
            }
        }
        swap(arr, start, arr.length - 1);
        return new String(arr);
    }
    public void swap(char[] arr, int x, int y) {
        while (x < y) {
            char tmp = arr[x];
            arr[x] = arr[y];
            arr[y] = tmp;
            x++;
            y--;
        }
    }
}
